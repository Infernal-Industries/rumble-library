# This is an example file. Copy to .env and fill in your values
# DO NOT commit the actual .env file to git

# Internet Archive credentials
# Get these from https://archive.org/account/s3.php
ARCHIVE_ACCESS_KEY=your_access_key_here
ARCHIVE_SECRET_KEY=your_secret_key_here

# Firebase configuration
# Get these from your Firebase Console -> Project Settings -> Web App
FIREBASE_API_KEY=your_api_key_here
FIREBASE_AUTH_DOMAIN=your-app.firebaseapp.com
FIREBASE_DATABASE_URL=https://your-app.firebaseio.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-app.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# Discord configuration
# Get these from Discord Developer Portal -> Your Application -> OAuth2
DISCORD_CLIENT_ID=your_client_id_here
DISCORD_CLIENT_SECRET=your_client_secret_here

# Server configuration
PORT=3000
NODE_ENV=development 